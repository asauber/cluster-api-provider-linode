{
  "Vendor": true,
  "Concurrency": 2,
  "DisableAll": true,
  "Skip": [
    "cloud/linode/providerconfig",
    "cloud/linode/actuators/machine",
    "cmd/cluster-controller",
    "cmd/machine-controller"
  ],
  "Exclude": [
    "should have comment or be unexported",
    "should have comment \\(or a comment on this block\\) or be unexported"
  ],
  "Deadline": "5m",
  "Enable": [
    "vet",
    "golint",
    "varcheck",
    "structcheck",
    "gosimple",
    "unused",
    "errcheck"
  ],
  "Linters": {
    "unused": {"Command": "unused -tags e2e"}
  }
}
